{
  "name": "game-of-life",
  "version": "1.0.0",
  "description": "A web implementation of conway's game of life",
  "main": "index.js",
  "scripts": {
    "lint": "eslint src/**/*.{js,vue} --fix",
    "prettier": "prettier --write **/*.{md,html,js,vue}",
    "dev": "NODE_ENV=development webpack serve",
    "test": "jest --coverage",
    "build": "webpack"
  },
  "keywords": [
    "gameoflife"
  ],
  "author": "Swann Castel",
  "license": "ISC",
  "devDependencies": {
    "@babel/preset-env": "^7.9.5",
    "babel-jest": "^25.4.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^6.7.3",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-vue": "^7.0.0-alpha.1",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^4.2.5",
    "jest": "^25.4.0",
    "lint-staged": "^10.1.7",
    "vue-loader": "^17.0.1",
    "vue-template-compiler": "^2.6.8",
    "webpack": "^5.1.2",
    "webpack-cli": "^4.0.0",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "vue": "^2.6.8"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,vue}": "eslint --fix",
    "*.{md,html,js,vue}": "prettier --write",
    "*.js": [
      "jest --findRelatedTests"
    ]
  }
}
